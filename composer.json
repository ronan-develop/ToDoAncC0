{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/monolog-bundle": "^3.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/flex": "^1.19",
        "symfony/twig-bundle": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.7",
        "symfony/asset": "^5.4",
        "symfony/mailer": "^5.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0"
    },
    "scripts": {
        "auto-scripts": [

        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "docker": false
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
